# =============================================================================
# ArmoGPT - AI-Powered Browser Automation Tool
# Environment Configuration File
# =============================================================================

# =============================================================================
# LLM PROVIDER CONFIGURATIONS
# =============================================================================
# Configure your preferred AI providers below. You only need to set up the
# providers you plan to use. Each provider requires an API key and optionally
# a custom endpoint URL.

# -----------------------------------------------------------------------------
# OpenAI Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_ENDPOINT=https://api.openai.com/v1
OPENAI_API_KEY=

# -----------------------------------------------------------------------------
# Anthropic (Claude) Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=
ANTHROPIC_ENDPOINT=https://api.anthropic.com

# -----------------------------------------------------------------------------
# Google (Gemini) Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=

# -----------------------------------------------------------------------------
# Azure OpenAI Configuration
# -----------------------------------------------------------------------------
# Get your credentials from: https://portal.azure.com/
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_API_VERSION=2025-01-01-preview

# -----------------------------------------------------------------------------
# DeepSeek Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://platform.deepseek.com/
DEEPSEEK_ENDPOINT=https://api.deepseek.com
DEEPSEEK_API_KEY=

# -----------------------------------------------------------------------------
# Mistral AI Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://console.mistral.ai/
MISTRAL_API_KEY=
MISTRAL_ENDPOINT=https://api.mistral.ai/v1

# -----------------------------------------------------------------------------
# Ollama Configuration (Local AI Models)
# -----------------------------------------------------------------------------
# Install Ollama from: https://ollama.ai/
# No API key required for local installation
OLLAMA_ENDPOINT=http://localhost:11434

# -----------------------------------------------------------------------------
# Alibaba (Qwen) Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://dashscope.console.aliyun.com/
ALIBABA_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1
ALIBABA_API_KEY=

# -----------------------------------------------------------------------------
# ModelScope Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://www.modelscope.cn/
MODELSCOPE_ENDPOINT=https://api-inference.modelscope.cn/v1
MODELSCOPE_API_KEY=

# -----------------------------------------------------------------------------
# MoonShot Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://platform.moonshot.cn/
MOONSHOT_ENDPOINT=https://api.moonshot.cn/v1
MOONSHOT_API_KEY=

# -----------------------------------------------------------------------------
# Unbound AI Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://www.getunbound.ai/
UNBOUND_ENDPOINT=https://api.getunbound.ai
UNBOUND_API_KEY=

# -----------------------------------------------------------------------------
# SiliconFlow Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://siliconflow.cn/
SiliconFLOW_ENDPOINT=https://api.siliconflow.cn/v1/
SiliconFLOW_API_KEY=

# -----------------------------------------------------------------------------
# IBM Watson Configuration
# -----------------------------------------------------------------------------
# Get your credentials from: https://cloud.ibm.com/
IBM_ENDPOINT=https://us-south.ml.cloud.ibm.com
IBM_API_KEY=
IBM_PROJECT_ID=

# -----------------------------------------------------------------------------
# Grok (Groq) Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://console.groq.com/
GROQ_ENDPOINT=https://api.groq.com/openai/v1
GROQ_API_KEY=

# Vercel AI Gateway Configuration
# -----------------------------------------------------------------------------
# Get your API key from: https://vercel.com/docs/ai-sdk
AI_GATEWAY_ENDPOINT=https://ai-gateway.vercel.sh/v1
AI_GATEWAY_API_KEY=

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================

# -----------------------------------------------------------------------------
# Default LLM Provider
# -----------------------------------------------------------------------------
# Set your preferred default LLM provider from the configured options above
# Available options: openai, anthropic, google, azure_openai, deepseek, mistral,
#                   ollama, alibaba, modelscope, moonshot, unbound, siliconflow,
#                   ibm, grok
DEFAULT_LLM=openai

# -----------------------------------------------------------------------------
# Telemetry Settings
# -----------------------------------------------------------------------------
# Set to false to disable anonymized telemetry collection
ANONYMIZED_TELEMETRY=false

# -----------------------------------------------------------------------------
# Logging Configuration
# -----------------------------------------------------------------------------
# LogLevel options: result | debug | info
# - result: Show only final results
# - debug: Enable verbose logging for troubleshooting
# - info: Standard logging level (recommended)
BROWSER_USE_LOGGING_LEVEL=info

# =============================================================================
# BROWSER AUTOMATION SETTINGS
# =============================================================================

# -----------------------------------------------------------------------------
# Browser Configuration
# -----------------------------------------------------------------------------
# Path to your browser executable (leave empty for auto-detection)
BROWSER_PATH=

# Custom user data directory for browser sessions
BROWSER_USER_DATA=

# Chrome DevTools Protocol settings
BROWSER_DEBUGGING_PORT=9222
BROWSER_DEBUGGING_HOST=localhost

# Set to true to keep browser open between AI tasks (recommended for development)
KEEP_BROWSER_OPEN=true

# Set to true to use your existing browser instance
USE_OWN_BROWSER=false

# Custom Chrome DevTools Protocol endpoint
BROWSER_CDP=

# -----------------------------------------------------------------------------
# Display Settings
# -----------------------------------------------------------------------------
# Screen resolution settings for browser automation
# Format: WIDTHxHEIGHTxDEPTH
RESOLUTION=1920x1080x24

# Individual resolution components
RESOLUTION_WIDTH=1920
RESOLUTION_HEIGHT=1080

# -----------------------------------------------------------------------------
# VNC Settings (for remote access)
# -----------------------------------------------------------------------------
# Password for VNC access (change this for security)
VNC_PASSWORD=youvncpassword
